<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 设置别名,否则在参数列表或者返回值类型这里,需要些类全名 -->
	<typeAliases>
		<!-- 对指定包下所有类设置别名,别名就是类名(首字母小写或大写都行) -->
		<package name="com.tledu.model" />
	</typeAliases>

	<!-- 映射文件 -->
	<mappers>
		<!-- 找到对应的xml -->
		<mapper resource="com/tledu/mapper/LoginLog.xml" />
		<mapper resource="com/tledu/mapper/SidebarSup.xml" />
		<mapper resource="com/tledu/mapper/User.xml" />
		<mapper resource="com/tledu/mapper/OperationLog.xml" />
		<mapper resource="com/tledu/mapper/Branch.xml" />
		<mapper resource="com/tledu/mapper/Dept.xml" />
		<mapper resource="com/tledu/mapper/Role.xml" />
		<mapper resource="com/tledu/mapper/MySchedule.xml" />
		<mapper resource="com/tledu/mapper/MyNote.xml" />
		<mapper resource="com/tledu/mapper/News.xml" />
		<mapper resource="com/tledu/mapper/Notices.xml" />
		
	</mappers>
</configuration>